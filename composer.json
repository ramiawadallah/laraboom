{
    "name": "ramiawadallah/laraboom",
    "description": "Laravel Admin package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel multiauth",
        "multiauth",
        "admin auth",
        "multi auth"
    ],
    "authors": [{
        "name": "Rami Awadallah",
        "email": "rami.moh.awadallah@gmail.com"
    }],
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Laraboom\\Multiauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraboom\\Multiauth\\Tests\\":"tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraboom\\Multiauth\\MultiauthServiceProvider"
            ]
        }
    }
